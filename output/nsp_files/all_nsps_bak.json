{
    "gateway-network-policy": {
        "PolicyName": "gateway-network-policy",
        "Namespace": "default",
        "PodSelector": {
            "matchLabels": {
                "app": "api-gateway"
            }
        },
        "PolicyTypes": [
            "Ingress",
            "Egress"
        ],
        "Ingress": [
            {
                "from": [
                    {
                        "podSelector": {
                            "matchLabels": {
                                "app": "webapp"
                            }
                        }
                    }
                ]
            }
        ],
        "Egress": [
            {
                "to": [
                    {
                        "podSelector": {
                            "matchLabels": {
                                "app": "staff-service"
                            }
                        }
                    },
                    {
                        "podSelector": {
                            "matchLabels": {
                                "app": "position-tracker"
                            }
                        }
                    }
                ]
            }
        ]
    },
    "position-simulator-network-policy": {
        "PolicyName": "position-simulator-network-policy",
        "Namespace": "fleetman",
        "PodSelector": {
            "matchLabels": {
                "app": "position-simulator"
            }
        },
        "PolicyTypes": [
            "Egress"
        ],
        "Ingress": "None",
        "Egress": [
            {
                "ports": [
                    {
                        "port": 8080,
                        "protocol": "TCP"
                    }
                ],
                "to": [
                    {
                        "podSelector": {
                            "matchLabels": {
                                "app": "staff-service"
                            }
                        }
                    },
                    {
                        "podSelector": {
                            "matchLabels": {
                                "app": "position-simulator"
                            }
                        }
                    }
                ]
            }
        ]
    },
    "position-tracker-network-policy": {
        "PolicyName": "position-tracker-network-policy",
        "Namespace": "fleetman",
        "PodSelector": {
            "matchLabels": {
                "app": "position-tracker"
            }
        },
        "PolicyTypes": [
            "Ingress",
            "Egress"
        ],
        "Ingress": [
            {
                "from": [
                    {
                        "podSelector": {
                            "matchLabels": {
                                "app": "api-gateway"
                            }
                        }
                    },
                    {
                        "podSelector": {
                            "matchLabels": {
                                "app": "position-simulator"
                            }
                        }
                    }
                ]
            }
        ],
        "Egress": [
            {
                "to": [
                    {
                        "podSelector": {
                            "matchLabels": {
                                "app": "staff-service"
                            }
                        }
                    },
                    {
                        "podSelector": {
                            "matchLabels": {
                                "app": "vehicle-telemetry"
                            }
                        }
                    }
                ]
            }
        ]
    },
    "staff-service-network-policy": {
        "PolicyName": "staff-service-network-policy",
        "Namespace": "fleetman",
        "PodSelector": {
            "matchLabels": {
                "app": "staff-service"
            }
        },
        "PolicyTypes": [
            "Ingress",
            "Egress"
        ],
        "Ingress": [
            {
                "from": [
                    {
                        "podSelector": {
                            "matchLabels": {
                                "app": "position-tracker"
                            }
                        }
                    },
                    {
                        "podSelector": {
                            "matchLabels": {
                                "app": "api-gateway"
                            }
                        }
                    }
                ]
            }
        ],
        "Egress": "Deny"
    },
    "vehicle-telemetry-network-policy": {
        "PolicyName": "vehicle-telemetry-network-policy",
        "Namespace": "fleetman",
        "PodSelector": {
            "matchLabels": {
                "app": "vehicle-telemetry"
            }
        },
        "PolicyTypes": [
            "Ingress",
            "Egress"
        ],
        "Ingress": [
            {
                "from": [
                    {
                        "podSelector": {
                            "matchLabels": {
                                "app": "position-tracker"
                            }
                        }
                    }
                ]
            }
        ],
        "Egress": "Deny"
    }
}